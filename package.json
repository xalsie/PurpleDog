{
  "name": "purpledog",
  "version": "0.0.1",
  "description": "PurpleDog - Hackaton project",
  "private": true,
  "scripts": {
    "install:api": "cd api && npm install",
    "install:front": "cd front && npm install",
    "install:all": "npm run install:api && npm run install:front",
    "run:dev:api": "cd api && npm run start:dev",
    "run:dev:front": "cd front && npm run dev",
    "dev:api": "npm run install:api && npm run dev:up && npm run run:dev:api",
    "dev:front": "npm run install:front && npm run run:dev:front",
    "dev:down": "docker-compose down -v",
    "dev:up": "docker-compose up -d --build",
    "dev:logs": "docker-compose logs -f",
    "prod:build": "docker-compose -f docker-compose.prod.yaml build",
    "prod:up": "docker-compose -f docker-compose.prod.yaml up -d --build",
    "prod:down": "docker-compose -f docker-compose.prod.yaml down",
    "logs:prod": "docker-compose -f docker-compose.prod.yaml logs -f"
  },
  "keywords": [
    "hackaton",
    "purpledog"
  ],
  "author": "",
  "license": "UNLICENSED"
}
